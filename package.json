{
  "name": "callbackurl",
  "version": "1.0.0",
  "description": "Generate instant live callback URLs for webhook testing. Forward, log, inspect, and replay incoming requests.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "callback": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "webhook",
    "callback",
    "tunnel",
    "proxy",
    "ngrok",
    "requestbin",
    "cli",
    "developer-tools"
  ],
  "author": "dwayne",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "better-sqlite3": "^12.6.2",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "http-proxy": "^1.18.1",
    "ink": "^6.8.0",
    "ink-table": "^3.1.0",
    "nanoid": "^3.3.11",
    "ora": "^9.3.0",
    "react": "^19.2.4",
    "selfsigned": "^5.5.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/http-proxy": "^1.17.17",
    "@types/node": "^25.3.2",
    "@types/react": "^19.2.14",
    "typescript": "^5.9.3"
  }
}
